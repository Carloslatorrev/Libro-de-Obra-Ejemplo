<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LODapp.Views.EnviarCorreoSoporte">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Enviar Correo a Soporte"
                 />
            <Label Text="Asunto"
                 />
            <Entry Placeholder="Asunto"/>
            <Label Text="Cuerpo"
                 />
            <Editor Placeholder="Cuerpo del Mensaje"/>
            <Button Text="Enviar" BackgroundColor="Green" CornerRadius="10"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>